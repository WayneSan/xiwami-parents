<br>
<div class="container">
<div class="panel panel-default">
  <div class="panel-heading">
  <div class="row">
     <label class="col-sm-2">Question Title: </label>
     <p class="col-sm-10">{{model.title}}</p>
  </div>
  </div>
  <div class="panel-body">
    <div class="row">
    <span class="col-sm-2">Description: </span>
      <p class="col-sm-10">{{model.description}}</p>
    </div>
    <div class="row">
    <span class="col-sm-2">Create Date: </span>
      <p class="col-sm-10">{{model.createdDate}}</p>
    </div>
    <div class="row">
    <span class="col-sm-2">Status:  </span>
      <p class="col-sm-10"> {{model.status}}</p>
    </div>
  </div>
  <div class="panel-footer">
    {{link-to 'Go Back' 'questions.my' class="btn btn-default"}}
    {{link-to 'Edit' 'questions.edit' model.id class='btn btn-cys'}}
  </div>
 </div>
</div>
